{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["jest"],
  "ignorePatterns": ["migrations/**"],
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "no-await-in-loop": "off"
  }
}
